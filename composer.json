{
  "name": "mavlitov98/clickhouse-migrations",
  "type": "symfony-bundle",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "fp4php/functional": "^5.0",
    "smi2/phpclickhouse": "^1.3"
  },
  "require-dev": {
    "fp4php/psalm-toolkit": "^3.2",
    "vimeo/psalm": "^4.9",
    "symfony/dependency-injection": "^6.0",
    "symfony/config": "^6.0",
    "symfony/http-kernel": "^6.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "ClickhouseMigrations\\": "/src"
    }
  }
}